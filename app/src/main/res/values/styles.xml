<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="spinnerWhite">
        <item name="android:fontFamily">@font/opensans_semibold</item>
        <item name="android:textColor">@color/colorBluePrimary</item>
    </style>

    <style name="fabWhite" parent="Widget.AppCompat.Button.Colored">
        <item name="icon">@drawable/ic_add</item>
        <item name="iconTint">@color/colorWhitePrimary</item>
        <item name="backgroundTint">@color/colorBluePrimary</item>
        <item name="cornerRadius">0dp</item>
    </style>
</resources>