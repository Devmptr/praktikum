<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kelompokv.praktikum">
    <!-- Internet -->
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".activity.user.DataKeluargaActivity"></activity>
        <activity android:name=".activity.user.ProfileActivity" />
        <activity android:name=".activity.FirebaseActivity" />
        <activity android:name=".activity.admin.anggota.ShowAnggotaActivity" />
        <activity android:name=".activity.admin.anggota.CreateAnggotaActivity" />
        <activity android:name=".activity.admin.anggota.IndexAnggotaActivity" />
        <activity android:name=".activity.admin.keluarga.CreateKeluargaActivity" />
        <activity android:name=".activity.admin.keluarga.ShowKeluargaActivity" />
        <activity android:name=".activity.admin.keluarga.IndexKeluargaActivity" />
        <activity android:name=".activity.admin.user.CreateUserActivity" />
        <activity android:name=".activity.admin.user.ShowUserActivity" />
        <activity android:name=".activity.admin.user.IndexUserActivity" />
        <activity android:name=".activity.user.FirstLoginActivity" />
        <activity android:name=".activity.admin.DashboardAdminActivity" />
        <activity android:name=".activity.user.ViewAnggota" />
        <activity android:name=".activity.user.CreateData" />
        <activity android:name=".activity.user.MainActivity" />
        <activity android:name=".activity.RegisterActivity" />
        <activity android:name=".activity.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service android:name=".network.service.FirebaseService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>
    </application>

</manifest>